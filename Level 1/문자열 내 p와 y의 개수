# 내 풀이
def solution(s):
    p_count = 0
    y_count = 0
    for i in s:
        if i in("p", "P"):
            p_count +=1
        elif i in("y","Y"):
            y_count += 1
        
    if p_count == y_count:
        return True
    else:
        return False
     
# 다른 사람의 풀이
def solution(s):
    return s.lower().count('p') == s.lower().count('y')
